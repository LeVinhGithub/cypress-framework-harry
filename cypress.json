{
  "ignoreTestFiles": ["**/__snapshots__/*", "**/__image_snapshots__/*"],
  "env": {
    "cypress-plugin-snapshots": {
      "imageConfig": {
        "threshold": 0.01
      }
    }
  },
  "pageLoadTimeout": 120000,
  "defaultCommandTimeout": 30000,
  "testFiles": "**/*spec.ts",
  "integrationFolder": "cypress/integration"
}
